<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zodiachoroscopes">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@drawable/horos_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/horos_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZodiacHoroscopes">
        <activity android:name=".ShowHistory" />
        <activity android:name=".CheckZodiac" />
        <activity android:name=".MainPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>